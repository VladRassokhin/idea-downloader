<project name="idea-downloader-idea-plugin-common" default="nop" basedir=".">
    <import file="build.idea.common.xml"/>

    <!-- parameters -->
    <property name="idea-plugin-build-type" value="bt"/>
    <property name="idea-plugin-build-number" value="latest.lastSuccessful"/>
    <property name="idea-plugin-build-server" value="???"/>
    <property name="idea-plugin-filename" value=".*"/>
    <property name="idea-plugin-build-branch" value="" />

    <!-- internals -->
    <property name="idea-plugin"        location="${idea-temp}/plugins"/>
    <property name="idea-plugin-temp"   location="${idea-plugin}/${idea-plugin-build-type}-${idea-plugin-build-number}"/>
    <property name="idea-plugin-unpack" location="${idea-plugin-temp}/unpack"/>
    
</project>
